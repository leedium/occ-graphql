{
  "name": "occ-graphql",
  "version": "1.1.0",
  "description": "Starter to help build and deploy Server Side Extensions to Oracle Commerce Cloud",
  "main": "app/index.js",
  "scripts": {
    "eslint": "eslint --ignore-path \".eslintignore\" -c \"../.eslintrc.json\" ../sse/**",
    "token": "node core/deploy.js",
    "test": "cross-env NODE_ENV=test mocha \"./{,!(node_modules)/**/}*.test.js\" --reporter spec -w",
    "test:nowatch": "cross-env NODE_ENV=test mocha \"./{,!(node_modules)/**/}*.test.js\" --reporter spec",
    "start-dev": "cross-env NODE_ENV=development nodemon -w ./index.js",
    "start-dev-local": "cross-env NODE_ENV=development nodemon -w .",
    "start-profile": "cross-env NODE_ENV=development node --prof index.js",
    "package": "npm-run-all package:init package:clean package:zip",
    "package:install": "npm i",
    "package:init": "shx --silent touch dummy.zip",
    "package:clean": "shx --silent rm *.zip",
    "package:shx:mkdir": "shx mkdir ../tmp",
    "package:shx:copytmp": "shx cp -r . ../tmp",
    "package:shx:cdtmp": "cd ../tmp && shx rm -rf node_modules && shx rm -rf tests && rm index.js && npm i --production && npm run package:deploy:no:eslint:test && cd .. && shx rm -rf ./tmp",
    "package:shx:lstmp": "shx ls",
    "package:clean:npm": "npm uninstall --save-dev express body-parser eslint esslint-config-standard eslint-plugin-import eslint-plugin-node eslint-plugin-promise eslint-plugin-standard expect.js mocha form-data nconf nodemon supertest yargs",
    "deploy": "npm-run-all eslint test:nowatch package:shx:mkdir package:shx:copytmp package:shx:cdtmp",
    "package:zip": "bestzip occgraphql-sse.zip ./* -x \"*.DS_Store\"",
    "package:deploy": "npm-run-all eslint package:init package:clean package:zip package:upload",
    "package:deploy:no:eslint:test": "npm-run-all package:init package:clean package:zip package:upload",
    "package:upload": "node core/deploy.js --token=$TOKEN"
  },
  "postinstall": "echo done",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leedium/occ-graphql.git"
  },
  "author": "LEEDIUM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leedium/occ-graphql/issues"
  },
  "homepage": "https://github.com/leedium/occ-graphql",
  "devDependencies": {
    "body-parser": "1.18.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "express": "4.16.3",
    "form-data": "^2.3.2",
    "mocha": "^5.1.1",
    "nconf": "^0.10.0",
    "nodemon": "^1.17.4",
    "supertest": "^3.1.0",
    "winston": "^3.0.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bestzip": "1.1.4",
    "cors": "2.8.4",
    "cross-env": "5.1.5",
    "expect.js": "^0.3.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "https-proxy-agent": "2.2.3",
    "node-fetch": "^2.6.0",
    "npm-run-all": "4.1.3",
    "request": "2.86.0",
    "shx": "0.2.2",
    "yargs": "11.0.0"
  },
  "publicUrls": [
    "/ccstorex/custom/v1/",
    "/ccstorex/custom/v1/occgraphql/version",
    "/ccstorex/custom/v1/occgraphql/test",
    "/ccstorex/custom/v1/occgraphql/graph"
  ],
  "authenticatedUrls": [
    {
      "url": "/ccstorex/custom/v1/occgraphql/authenticatedUrl",
      "method": [
        "get"
      ]
    },
    {
      "url": "/ccstorex/custom/v1/occgraphql/secure",
      "method": [
        "post"
      ]
    }
  ],
  "translations": [
    {
      "language": "en_US",
      "description": "Testing graphql with occ",
      "name": "OCC GRAPH QL"
    }
  ]
}
